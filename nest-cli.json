{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/hororok-sns-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/hororok-sns-server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/hororok-sns-server",
  "projects": {
    "hororok-sns-server": {
      "type": "application",
      "root": "apps/hororok-sns-server",
      "entryFile": "main",
      "sourceRoot": "apps/hororok-sns-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hororok-sns-server/tsconfig.app.json"
      }
    },
    "timer-app": {
      "type": "application",
      "root": "apps/timer-app",
      "entryFile": "main",
      "sourceRoot": "apps/timer-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/timer-app/tsconfig.app.json"
      }
    }
  }
}
